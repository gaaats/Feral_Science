    <fragment
        android:id="@+id/aboutAppInfoFragment"
        android:name="com.quizapp.quizfoirtopics.AboutAppInfoFragment"
        android:label="fragment_about_app_info"
        tools:layout="@layout/fragment_about_app_info" />
    <fragment
        android:id="@+id/someSettingsFragment"
        android:name="com.quizapp.quizfoirtopics.SomeSettingsFragment"
        android:label="fragment_some_settings"
        tools:layout="@layout/fragment_some_settings" />
