            android:id="@+id/action_enterFragment_to_mathematicsFragment"
            app:enterAnim="@anim/from_right"
            app:exitAnim="@anim/to_left"
            app:popEnterAnim="@anim/from_left"
            app:popExitAnim="@anim/to_right"
            app:destination="@id/mathematicsFragment" />
        <action
            android:id="@+id/action_enterFragment_to_sportsFragment"
            app:enterAnim="@anim/from_right"
            app:exitAnim="@anim/to_left"
            app:popEnterAnim="@anim/from_left"
            app:popExitAnim="@anim/to_right"
            app:destination="@id/sportsFragment" />
        <action
            android:id="@+id/action_enterFragment_to_someSettingsFragment"
            app:enterAnim="@anim/from_right"
            app:exitAnim="@anim/to_left"
            app:popEnterAnim="@anim/from_left"
            app:popExitAnim="@anim/to_right"
            app:destination="@id/someSettingsFragment" />
        <action
            android:id="@+id/action_enterFragment_to_musicFragment"
            app:enterAnim="@anim/from_right"
            app:exitAnim="@anim/to_left"
            app:popEnterAnim="@anim/from_left"
            app:popExitAnim="@anim/to_right"
            app:destination="@id/musicFragment" />
        <action
            android:id="@+id/action_enterFragment_to_geographyFragment"
            app:enterAnim="@anim/from_right"
            app:exitAnim="@anim/to_left"
