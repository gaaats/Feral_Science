            app:popEnterAnim="@anim/from_left"
            app:popExitAnim="@anim/to_right"
            app:destination="@id/aboutAppInfoFragment" />
    </fragment>
    <fragment
        android:id="@+id/sportsFragment"
        android:name="com.quizapp.quizfoirtopics.SportsFragment"
        android:label="fragment_sports"
        tools:layout="@layout/fragment_sports" >
        <action
            android:id="@+id/action_sportsFragment_to_correctAnsFragment"
            app:enterAnim="@anim/from_right"
            app:exitAnim="@anim/to_left"
            app:popEnterAnim="@anim/from_left"
            app:popExitAnim="@anim/to_right"
            app:destination="@id/correctAnsFragment" />
        <action
            android:id="@+id/action_sportsFragment_to_vrongAnsFragment"
            app:enterAnim="@anim/from_right"
            app:exitAnim="@anim/to_left"
