            app:popEnterAnim="@anim/from_left"
            app:popExitAnim="@anim/to_right"
            app:destination="@id/aboutAppInfoFragment" />
    </fragment>
    <fragment
        android:id="@+id/mathematicsFragment"
        android:name="com.quizapp.quizfoirtopics.MathematicsFragment"
        android:label="fragment_mathematics"
        tools:layout="@layout/fragment_mathematics" >
        <action
            android:id="@+id/action_mathematicsFragment_to_correctAnsFragment"
            app:enterAnim="@anim/from_right"
            app:exitAnim="@anim/to_left"
            app:popEnterAnim="@anim/from_left"
            app:popExitAnim="@anim/to_right"
            app:destination="@id/correctAnsFragment" />
        <action
            android:id="@+id/action_mathematicsFragment_to_vrongAnsFragment"
            app:enterAnim="@anim/from_right"
