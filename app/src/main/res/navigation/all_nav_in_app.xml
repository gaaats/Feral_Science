            app:popExitAnim="@anim/to_right"
            app:destination="@id/geographyFragment" />
        <action
            android:id="@+id/action_enterFragment_to_aboutAppInfoFragment"
            app:enterAnim="@anim/from_right"
            app:exitAnim="@anim/to_left"
            app:popEnterAnim="@anim/from_left"
            app:popExitAnim="@anim/to_right"
            app:destination="@id/aboutAppInfoFragment" />
    </fragment>
    <fragment
        android:id="@+id/geographyFragment"
        android:name="com.quizapp.quizfoirtopics.GeographyFragment"
        android:label="fragment_geography"
        tools:layout="@layout/fragment_geography" >
        <action
            android:id="@+id/action_geographyFragment_to_correctAnsFragment"
