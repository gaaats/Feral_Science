        android:id="@+id/correctAnsFragment"
        android:name="com.quizapp.quizfoirtopics.CorrectAnsFragment"
        android:label="fragment_correct_ans"
        tools:layout="@layout/fragment_correct_ans" >
        <action
            android:id="@+id/action_correctAnsFragment_to_geographyFragment"
            app:destination="@id/geographyFragment"
            app:enterAnim="@anim/from_right"
            app:exitAnim="@anim/to_left"
            app:launchSingleTop="true"
            app:popEnterAnim="@anim/from_left"
            app:popExitAnim="@anim/to_right"
            app:popUpToInclusive="true" />
        <action
            android:id="@+id/action_correctAnsFragment_to_mathematicsFragment"
            app:enterAnim="@anim/from_right"
            app:exitAnim="@anim/to_left"
            app:launchSingleTop="true"
            app:popEnterAnim="@anim/from_left"
            app:popExitAnim="@anim/to_right"
            app:popUpToInclusive="true"
            app:destination="@id/mathematicsFragment" />
        <action
            android:id="@+id/action_correctAnsFragment_to_sportsFragment"
            app:enterAnim="@anim/from_right"
            app:exitAnim="@anim/to_left"
            app:launchSingleTop="true"
            app:popEnterAnim="@anim/from_left"
            app:popExitAnim="@anim/to_right"
            app:popUpToInclusive="true"
            app:destination="@id/sportsFragment" />
        <action
            android:id="@+id/action_correctAnsFragment_to_musicFragment"
            app:enterAnim="@anim/from_right"
            app:exitAnim="@anim/to_left"
            app:launchSingleTop="true"
            app:popEnterAnim="@anim/from_left"
            app:popExitAnim="@anim/to_right"
            app:popUpToInclusive="true"
            app:destination="@id/musicFragment" />
        <argument
            android:name="type_quiz"
            app:argType="integer" />
        <action
            android:id="@+id/action_correctAnsFragment_to_enterFragment"
            app:enterAnim="@anim/from_left"
            app:exitAnim="@anim/to_right"
            app:popEnterAnim="@anim/from_left"
            app:popExitAnim="@anim/to_right"
            app:destination="@id/enterFragment" />
    </fragment>
    <fragment
        android:id="@+id/vrongAnsFragment"
        android:name="com.quizapp.quizfoirtopics.VrongAnsFragment"
        android:label="fragment_vrong_ans"
        tools:layout="@layout/fragment_vrong_ans" >
        <action
            android:id="@+id/action_vrongAnsFragment_to_musicFragment"
            app:enterAnim="@anim/from_left"
            app:exitAnim="@anim/to_right"
            app:launchSingleTop="true"
            app:popEnterAnim="@anim/from_left"
